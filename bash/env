export PATH=/opt/local/bin:/opt/local/sbin:/usr/local/share/npm/bin:~/bin:/usr/local/sbin:$PATH
export EDITOR=/usr/bin/vim
export CLICOLOR=1
#export LSCOLORS="DxfxcxdxbxegDbabagacad"
export NODE_PATH="/usr/local/lib/node/"
export NODE_ENV="development"
export HISTIGNORE="&:ls:[bf]g:exit:h"

bind "set completion-ignore-case on"
set -o vi

if [[ -s /Users/ryan/.rvm/scripts/rvm ]] ; then source /Users/ryan/.rvm/scripts/rvm ; fi

if [ -f `brew --prefix`/etc/bash_completion ]; then
  . `brew --prefix`/etc/bash_completion
fi

